import java.util.*;

class myPair{
	public int first;
	public int second;
	
	public myPair(int a, int b){
		this.first = a;
		this.second = b;
	}

}

public class evaluateExp {
	/*Given expression with operands and operators (OR , AND , XOR) , in how many ways can you evaluate the
	 expression to true, by grouping in different ways? Operands are only true and false.*/
	
	int mod = 1003;
	HashMap<myPair, Integer> _true, _false;
	
	public evaluateExp() {
		// TODO Auto-generated constructor stub
		_true = new HashMap<>();
		_false = new HashMap<>();
	}
	
	public int evTrue(int i, int j, String a){
		if (i > j)
			return 0;
		
		if (i == j)
			return a.charAt(i) == 'T' ? 1 : 0;
		
//		if (_true)
		return 0;
		
	}
	
	public int evFalse() {
		
		return 0;
	}
	
	public int cnttrue(String a) {
		
		_true.clear();
		_false.clear();
		
		int n = a.length();
		int T[] = new int[n];
		int F[] = new int[n];
		
		return evTrue(0, n -1, a);
	}

	public static void main(String[] args) {
		System.out.println(new evaluateExp().cnttrue("T|F&T^T"));
	}
	
}
